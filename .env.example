
# MongoDB Configuration
# Root user credentials for MongoDB admin
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=your_strong_password_here

# Initial database name (optional)
MONGO_DATABASE=your_database_name

# Mongo Express Configuration
# Web UI credentials
MONGO_EXPRESS_USERNAME=admin
MONGO_EXPRESS_PASSWORD=your_express_password_here

# Mongo Express port (optional, defaults to 8081 if not set)
MONGO_EXPRESS_PORT=8081